{
  "extends": "airbnb",
  "plugins": [ "jest" ],    
  "env": {
    "jest/globals": true,
    "browser": true,
    "node": true,
    "jquery": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "react/prop-types": [0],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true }]
  }
}